{
  "name": "jira-rest-mock-server",
  "version": "0.0.1",
  "description": "An Express app that provides a search route with a delay.",
  "main": "src/server/app.ts",
  "scripts": {
    "start": "ts-node src/server/app.ts",
    "build": "tsc",
    "nodemon": "nodemon src/server/app.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0"
  },
  "author": "",
  "license": "MIT",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  }
}
